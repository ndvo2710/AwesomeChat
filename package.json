{
  "name": "awesomechat",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s --copy-files",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js",
    "dev": "babel-node src/app.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-ejs-extend": "0.0.1",
    "express-validator": "^6.4.0",
    "http-errors": "~1.6.3",
    "log4js": "^6.1.2",
    "mongoose": "^5.9.7",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "nodemon": "^2.0.2"
  }
}
